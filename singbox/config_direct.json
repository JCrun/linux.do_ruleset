{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "type": "https",
        "tag": "Local-DNS",
        "domain_resolver": "Local-DNS-Resolver",
        "server": "sing.ddd.oaifree.com",
        "server_port": 443,
        "path": "/query-dns"
      },
      {
        "type": "udp",
        "tag": "Local-DNS-Resolver",
        "server": "223.5.5.5",
        "server_port": 53
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "Local-DNS"
      },
      {
        "clash_mode": "global",
        "server": "Local-DNS"
      },
      {
        "rule_set": "linux.do",
        "server": "Local-DNS"
      }
    ],
    "final": "Local-DNS",
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "mtu": 9000,
      "address": "172.18.0.1/30",
      "auto_route": true,
      "auto_redirect": true,
      "strict_route": true,
      "stack": "mixed"
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 20122,
      "udp_fragment": false
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "GLOBAL"
      },
      {
        "protocol": "quic",
        "action": "reject"
      },
      {
        "rule_set": "linux.do",
        "outbound": "direct",
        "tls_fragment_fallback_delay": "500ms",
        "tls_record_fragment": true
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "linux.do",
        "url": "https://raw.githubusercontent.com/JCrun/linux.do_ruleset/main/singbox/linux.do.srs",
        "download_detour": "direct"
      }
    ],
    "auto_detect_interface": true,
    "default_domain_resolver": "Local-DNS"
  }
}

